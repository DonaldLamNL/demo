eRoot":"src/components/homeSection","sources":["Skills.vue"],"sourcesContent":["<template>\n    <div id=\"skills\">\n        <h1>Technical Skills</h1>\n        <h2>I am good at the following technical</h2>\n        <div class=\"wrapper\" :style=\"'width:' + wrapperWidth + 'px'\">\n            <div class=\"container\" v-for=\"(i, index) in technical\" :key=\"index\">\n                <h3>{{i.name}}</h3>\n                <Progress\n                    class=\"progress-bar\"\n                    :value=\"i.progress\"\n                    :strokeWidth=\"10\"\n                    :radius=\"50\"\n                    :transitionDuration=\"2000\"\n                />\n                <div class=\"content\">\n                    <ul>\n                        <li v-for=\"(l, l_index) in i.content\" :key=\"l_index\">{{l}}</li>\n                    </ul>\n                </div>\n            </div>\n        </div>\n    </div>\n</template>\n\n<script>\nimport Progress from 'easy-circular-progress/src/index.vue'\n\nexport default {\n    name:'Skills',\n    components: { Progress },\n    data() {\n        return {\n            wrapperWidth: 0,\n            technical:[\n                {name: 'HTML', progress: 90, content: ['Tags', 'Lists', 'Tables', 'Forms']},\n                {name: 'CSS', progress: 50, content: ['Basic', 'Background', 'Layout', 'Float', 'Position']},\n                {name: 'JavaScript', progress: 70, content: ['Data Types', 'Function & Object', 'DOM & BOM', '', 'Regular Expression', 'ES5 Methods']},\n                {name: 'ES6-11', progress: 70, content: ['Destructuring Assignment', 'Template Literals', 'Arrow Function', 'Iterators & Generators', 'Promise', '']},\n                {name: 'Ajax', progress: 80, content: ['XML', 'JQuery', 'JSONP', 'CORS', 'fetch']},\n                {name: 'NodeJS', progress: 80, content: ['CommonJS', 'npm & yarn', 'Node Modules', 'express', 'Interact with MongoDB']},\n                {name: 'VueJS', progress: 70, content: ['Vue2', 