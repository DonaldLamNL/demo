currentRoute.path) {\n                case value:\n                    \n                    break;\n            \n                default:\n                    break;\n            }\n        }\n    },\n    mounted() {\n        window.addEventListener('scroll', this.onScroll)\n\n    },\n}\n</script>\n\n<style scoped>\n#nav{\n    text-align: center;\n    z-index: 9999;\n}\n.home-top{\n    height: 50px;\n    position: relative;\n    margin-bottom: -50px;\n    transition: height 0.5s;\n}\n.home-nav{\n    height: 50px;\n    position: fixed;\n    top: 0;\n    left: 0;\n    background-color: #bfa;\n}\n.nav-list{\n    display: inline-block;\n    width: fit-content;\n}\n.nav-item{\n    display: inline-block;\n    margin: 0 40px;\n}\n.nav-link{\n    display: block;\n    position: relative;\n    line-height: 50px;\n    font-size: 20px;\n    text-decoration: none;\n}\n.nav_bar{\n    width: 0;\n    height: 2px;\n    position: absolute;\n    bottom: 13px;\n    background-color: rgb(207, 207, 20);\n    transition: width 0.3s;\n}\n.active > .nav_bar,\n.nav-link:hover > .nav_bar{\n    width: 100%;\n}\n</style>"]},"metadata":{},"sourceType":"module"}